{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "/backend/*.csproj",
            "/frontend/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": false,
      "includePrivateMembers": true,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "arolariu.ro DocFX",
      "_enableSearch": "true",
      "_enableNewTab": "true"
    },
    "template": [
      "default",
      "modern"
    ],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://docs.arolariu.ro/",
      "priority": 0.5,
      "changefreq": "daily"
    }
  }
}
