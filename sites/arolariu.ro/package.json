{
  "name": "@arolariu/website",
  "description": "The `arolariu.ro` project consists of a Next.JS web application.",
  "homepage": "https://arolariu.ro",
  "repository": "github:arolariu/arolariu.ro",
  "version": "1.0.0",
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c",
  "private": true,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24",
    "yarn": ">=4.5"
  },
  "keywords": [
    "NextJS",
    "ReactJS",
    "TailwindCSS"
  ],
  "bugs": {
    "url": "https://github.com/arolariu/arolariu.ro/issues",
    "email": "admin@arolariu.ro"
  },
  "author": {
    "name": "Alexandru-Razvan Olariu",
    "email": "admin@arolariu.ro",
    "url": "https://arolariu.ro"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "npm run build:before && next build && npm run build:after",
    "build:before": "tsx scripts/beforeBuild.ts",
    "build:after": "tsx scripts/afterBuild.ts",
    "build:debug": "cross-env NODE_ENV=development next build --debug --profile --no-mangling",
    "check": "npm run check:translations && npm run check:acknowledgements",
    "check:acknowledgements": "tsx scripts/checkAcknowledgements.ts",
    "check:translations": "tsx scripts/checkTranslations.ts",
    "clean": "tsx scripts/clean.ts",
    "dev": "npm run clean && cross-env NODE_OPTIONS='--inspect' next dev --turbopack --experimental-https",
    "dev:classic": "npm run clean && cross-env NODE_OPTIONS='--inspect' next dev --experimental-https",
    "dev:debug": "npm run clean && cross-env NODE_ENV=development npm run build:debug && npm run start:debug",
    "email": "npm run clean && email dev",
    "format": "tsx scripts/format.ts",
    "generate": "npm run generate:translations && npm run generate:acknowledgements",
    "generate:acknowledgements": "tsx scripts/generateAcknowledgements.ts",
    "generate:environment": "tsx scripts/generateEnvironment.ts",
    "generate:translations": "tsx scripts/generateTranslations.ts",
    "lint": "eslint src/ --no-cache --config eslint.config.ts",
    "lint:fix": "eslint src/ --fix --config eslint.config.ts",
    "lint:inspect": "eslint --inspect-config",
    "setup": "npm run generate:environment",
    "setup:azure": "cross-env INFRA='azure' npm run setup",
    "setup:azure:verbose": "cross-env INFRA='azure' VERBOSE=true npm run setup",
    "setup:local": "cross-env INFRA='local' npm run setup",
    "setup:local:verbose": "cross-env INFRA='local' VERBOSE=true npm run setup",
    "start": "next start",
    "start:debug": "cross-env NODE_OPTIONS='--inspect' NODE_ENV=development node --enable-source-maps .next/standalone/server.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --coverage --coverageDirectory code-cov/storybook-report/ --no-cache --browsers firefox chromium webkit",
    "test": "npm run test:jest && npm run test:playwright && npm run test:storybook",
    "test:unit": "jest",
    "test:jest": "jest",
    "test:playwright": "playwright test --config=playwright.config.ts",
    "test:storybook": "npm run storybook:build && npm run storybook:test"
  },
  "dependencies": {
    "@arolariu/components": "0.1.2",
    "@azure/app-configuration": "1.9.0",
