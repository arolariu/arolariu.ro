{
  "name": "arolariu.ro",
  "description": "The `arolariu.ro` project consists of a Next.JS web application.",
  "homepage": "https://arolariu.ro",
  "repository": "github:arolariu/arolariu.ro",
  "version": "1.0.0",
  "packageManager": "yarn@4.8.1+sha512.bc946f2a022d7a1a38adfc15b36a66a3807a67629789496c3714dd1703d2e6c6b1c69ff9ec3b43141ac7a1dd853b7685638eb0074300386a59c18df351ef8ff6",
  "private": true,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=22.6",
    "yarn": ">=4.5"
  },
  "keywords": [
    "NextJS",
    "ReactJS",
    "TailwindCSS"
  ],
  "bugs": {
    "url": "https://github.com/arolariu/arolariu.ro/issues",
    "email": "admin@arolariu.ro"
  },
  "author": {
    "name": "Alexandru-Razvan Olariu",
    "email": "admin@arolariu.ro",
    "url": "https://arolariu.ro"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "npm run build:before && next build && npm run build:after",
    "build:before": "npx tsx scripts/beforeBuild.ts",
    "build:after": "npx tsx scripts/afterBuild.ts",
    "build:debug": "cross-env NODE_ENV=development next build --debug --profile --no-mangling",
    "check": "npm run check:translations && npm run check:acknowledgements",
    "check:acknowledgements": "npx tsx scripts/checkAcknowledgements.ts",
    "check:translations": "npx tsx scripts/checkTranslations.ts",
    "clean": "npx tsx scripts/clean.ts",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack --experimental-https",
    "dev:classic": "cross-env NODE_OPTIONS='--inspect' next dev --experimental-https",
    "dev:debug": "cross-env NODE_ENV=development npm run build:debug && npm run start:debug",
    "email": "email dev",
    "format": "npx tsx scripts/format.ts",
    "generate": "npm run generate:translations && npm run generate:acknowledgements",
    "generate:acknowledgements": "npx tsx scripts/generateAcknowledgements.ts",
    "generate:translations": "npx tsx scripts/generateTranslations.ts",
    "lint": "eslint src/ --no-cache --config eslint.config.ts",
    "lint:fix": "eslint src/ --fix --config eslint.config.ts",
    "lint:inspect": "eslint --inspect-config",
    "start": "next start",
    "start:debug": "cross-env NODE_OPTIONS='--inspect' NODE_ENV=development node --enable-source-maps .next/standalone/server.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --no-cache --browsers firefox chromium webkit",
    "test": "npm run test:jest && npm run test:playwright && npm run test:storybook",
    "test:jest": "jest",
    "test:playwright": "playwright test --config=playwright.config.ts",
    "test:storybook": "npm run storybook:test"
  },
  "dependencies": {
    "@arolariu/components": "0.0.35",
    "@azure/app-configuration": "1.9.0",
    "@azure/identity": "4.8.0",
