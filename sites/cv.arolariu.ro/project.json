{
  "name": "@arolariu/cv",
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "sourceRoot": "sites/cv.arolariu.ro",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "sites/cv.arolariu.ro"
      },
      "outputs": [
        "{projectRoot}/build"
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "sites/cv.arolariu.ro"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run preview",
        "cwd": "sites/cv.arolariu.ro"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run prepare",
        "cwd": "sites/cv.arolariu.ro"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run check",
        "cwd": "sites/cv.arolariu.ro"
      }
    },
    "check:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run check:watch",
        "cwd": "sites/cv.arolariu.ro"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "sites/cv.arolariu.ro"
      },
      "configurations": {
        "unit": {
          "command": "npm run test:unit"
        }
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run format",
        "cwd": "sites/cv.arolariu.ro"
      }
    }
  },
  "tags": [
    "domain:frontend",
    "scope:cv",
    "type:app"
  ]
}
