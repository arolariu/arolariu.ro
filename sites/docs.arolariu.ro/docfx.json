{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "src": "../../docs",
        "dest": "articles/monorepo"
      },
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "articles/monorepo/toc.yml",
          "_site/**"
        ]
      }
    ],
    "resource": [
      "images/**"
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "arolariu.ro DocFX",
      "_appName": "docs.arolariu.ro",
      "_appFooter": "<strong>Built using <em><a href=\"https://dotnet.github.io/docfx\">DocFX</a></em>.</strong>",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableContribution": false,
      "_disableSideFilter": false,
      "msClarityId": "hvhwmt03k3",
      "_gitContribute": {
        "repo": "https://github.com/arolariu/arolariu.ro",
        "branch": "main",
        "path": "/"
      },
      "_gitUrlPattern": "github"
    },
    "template": [
      "default",
      "modern",
      "templates/clarity"
    ],
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://docs.arolariu.ro/",
      "priority": 0.5,
      "changefreq": "weekly"
    },
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "tasklists",
        "diagrams"
      ]
    },
    "postProcessors": [
      "ExtractSearchIndex"
    ]
  },
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/src/**/*.csproj"
          ],
          "src": "../api.arolariu.ro/"
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": false,
      "disableDefaultFilter": false,
      "disableGitFeatures": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "EnumSortOrder": "declaringOrder",
      "includePrivateMembers": true,
      "includeExplicitInterfaceImplmenetations": true
    }
  ]
}
